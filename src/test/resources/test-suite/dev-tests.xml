<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Care Reminder Test Suite">
    <parameter name="browser" value="chrome" />
    <listeners>
        <listener class-name="com.moda.listeners.TestListener"/>
        <listener class-name="org.testng.reporters.EmailableReporter2"/>
        <listener class-name="org.testng.reporters.TestHTMLReporter"/>
        <listener class-name="com.moda.listeners.EmailReporter"/>
    </listeners>
    <test verbose="2" name="Care Reminder Test suite">
        <classes>
            <class name="com.moda.dev.CareReminderTestCase" >
                <methods>
                    <include name="validLoginTest"/>
                    <include name="careReminderTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>